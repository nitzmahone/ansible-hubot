{
  "name": "hubot",
  "version": "0.0.0",
  "private": true,
  "author": "Matt Davis <mdavis@ansible.com>",
  "description": "A simple helpful robot for your Company",
  "dependencies": {
    "express": "~3.4.4",
    "hubot-diagnostics": "0.0.1",
    "hubot-google-images": "^0.1.5",
    "hubot-google-translate": "^0.2.0",
    "hubot-help": "^0.2.0",
    "hubot-plusplus": "^1.3.0",
    "hubot-pugme": "^0.1.0",
    "hubot-rules": "^0.1.1",
    "hubot-scripts": "^2.16.0",
    "hubot-slack": "^4.1.0",
    "jobot-brain-file": "^1.0.1"
  },
  "engines": {
    "node": "0.10.x",
    "npm": ">= 1.0.0"
  },
  "scripts": {
    "start": "npm run start-express & npm run start-hubot",
    "start-express": "node server.js",
    "start-hubot": "PORT=$OPENSHIFT_NODEJS_PORT BIND_ADDRESS=OPENSHIFT_NODEJS_IP REDIS_URL=redis://:$REDISCLOUD_PASSWORD@$REDISCLOUD_HOSTNAME:$REDISCLOUD_PORT bin/hubot -n Hubot -a slack"
